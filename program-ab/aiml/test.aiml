<category><pattern>NESTED LOOP</pattern>
<template><think><set var="jcount">0</set></think>
<condition var="jcount">
<li value="8">
</li>
<li><get var="jcount"/>:<think><set var="jcount"><map name="successor"><get var="jcount"/></map></set>  <set var="icount">0</set></think>  <condition var="icount">
  <li value="8">
  </li>
  <li><get var="icount"/> <think><set var="icount"><map name="successor"><get var="icount"/></map></set></think> <loop/></li>
  </condition>
<loop/></li>
</condition></template>
</category>
